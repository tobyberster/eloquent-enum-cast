{
    "name": "orkhanahmadov/eloquent-enum-cast",
    "description": "Enum cast for Eloquent",
    "keywords": [
        "laravel",
        "eloquent",
        "eloquent-enum-cast"
    ],
    "homepage": "https://github.com/orkhanahmadov/eloquent-enum-cast",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "hi@orkhan.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "myclabs/php-enum": "^1.8"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\EloquentEnumCast\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\EloquentEnumCast\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
